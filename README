# Starting a fresh download
cd /supercell/bam_gt/active_downloads
rm -f *.log
rm -rf /supercell/tcga_downloads/coad
rm -f /supercell/bam_requests/active_downloads/bam_status_COAD_WGS.x*.tsv
./direct_parallel_prep.sh
./direct_parallel.sh &

# stopping and restarting a partial download
killall gtdownload; killall python
# check to make sure all gtdownload and python processes are stopped
./direct_parallel.sh &

